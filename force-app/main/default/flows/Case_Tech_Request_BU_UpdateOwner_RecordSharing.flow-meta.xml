    <decisions>
        <description>Based on Business Impact</description>
        <name>Case_Priority</name>
        <label>Case Priority?</label>
        <locationX>578</locationX>
        <locationY>1139</locationY>
        <defaultConnector>
            <targetReference>Update_Case_Pri_ProductLine_BU</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Level_1_Critical</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Business_Impact__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Critical - Application is down for ALL USERS</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Priority</targetReference>
            </connector>
            <label>Level 1 - Critical</label>
        </rules>
        <rules>
            <name>Level_2_Urgent</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Business_Impact__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>High - Serious degradation of application performance or functionality</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Priority2</targetReference>
            </connector>
            <label>Level 2 - Urgent</label>
        </rules>
        <rules>
            <name>Level_3_High</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Business_Impact__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Normal - Application issue that has moderate impact to the business</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Priority3</targetReference>
            </connector>
            <label>Level 3  - High</label>
        </rules>
        <rules>
            <name>Level_4_Medium</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Business_Impact__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Low - Issue or Question with limited business impact</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Priority4</targetReference>
            </connector>
            <label>Level 4 - Medium</label>
        </rules>
